{
  "version": 2,
  "projects": {
    "covis-app": "apps/covis-app",
    "covis-app-e2e": "apps/covis-app-e2e",
    "covis-converter": "apps/covis-converter",
    "covis-service": "apps/covis-service",
    "shared": "libs/shared",
    "shared-backend": "libs/shared-backend"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "generators": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "covis-app"
}
